    "jquery-mockjax": "~1.5.3",
    "jasmine-jquery": "~2.0.3",
    "jasmine-fixture": "~1.2.0"
  },

  "main": [
    "/dist/fullcalendar.js",
    "/dist/fullcalendar.css"
  ],
  "ignore": [
    "*",
